<html lang="ar" dir="rtl">
<head>
<meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
	<title>المستخدمين - Admin</title>

<meta name="viewport" content="width=device-width,initial-scale=1.0">
</head>
<body class="wp-admin wp-core-ui js edit-php auto-fold admin-bar post-type-project branch-5-4 version-5-4-2 admin-color-fresh locale-en-us customize-support svg">
<div id="wpwrap">


<div id="wpcontent">

<div id="wpbody" role="main">
<div id="wpbody-content">
		
				

		<div class="wrap" style="text-align: center; font-size: 20px;">
<h1 class="wp-heading-inline">المستخدمين</h1>
<br/><br/>
<div align="left"> 
 <a class="btn btn-primary" href="/users/new" >اضافة مستخدم</a>
</div>
 <br/> <br/>
 <br/>

 <form id="posts-filter" method="get">
     <input type="hidden" id="_wpnonce" name="_wpnonce" value="09d2d980db"><input type="hidden" name="_wp_http_referer" value="/single-sites/project/wp-admin/edit.php?post_type=project">	
         <table class="table table-bordered table-hover table-sortable" style="text-align: center; font-size: 16px;font-weight: bold; color:black">
             <thead>
                 <tr style="color:black">
                     <th scope="col" id="title" class="text-center"> اسم المستخدم</th>
                     <th scope="col" id="author" class="text-center">البريد الالكتروني</th>	
                     <th scope="col" id="options" class="text-center">العمليات</th>	
                    </tr>
                </thead>
                <% users.forEach(user => { %>
                    <tbody id="the-list">	
                        <tr>
                            <td > <strong><%= user.name %></strong> </td>
                                <td class="author column-author" data-colname="Author"><%= user.email %></td>
                                <td>
                                    <div ><a class="awesome" href="/users/<%= user._id %>/update">تعديل</a> 
                                        <a class="awesome" href="/users/<%= user._id%>/show"target="_blank">عرض</a>
                                        <a class="trash" href="/users/<%= user._id %>/delete?_method=DELETE" >حذف</a>
                                    </div>
                                </td>
                            </tr>
                            
                        </tbody>
                        <%})%>        
                    </table>	
                </form>
            </div>
        </div>
    </div>
    </div>
    </div>
    </body>
    </html>